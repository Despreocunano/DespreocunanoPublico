{
  "rewrites": [
    {
      "source": "/invitacion/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/regalos/:slug",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/((?!invitacion/|regalos/).*)",
      "destination": "https://app.lovio.cl/$1",
      "permanent": false
    }
  ]
}
