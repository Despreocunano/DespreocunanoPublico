
{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/invitacion/:slug",
      "destination": "https://app.lovio.cl/invitacion/:slug"
    },
    {
      "source": "/regalos/:slug",
      "destination": "https://app.lovio.cl/regalos/:slug"
    },
    {
      "source": "/assets/:file*",
      "destination": "https://app.lovio.cl/assets/:file*"
    },
    {
      "source": "/_astro/:file*",
      "destination": "https://app.lovio.cl/_astro/:file*"
    }
  ]
}